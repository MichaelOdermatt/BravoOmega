            <string>///check if start is ready to be changed
if  (start==0) {
    if (obj_menuTxt.txtit==obj_menuTxt.txt) {
        start = 1;
    }
} else {
    if  gend==0&amp;&amp;pref==0&amp;&amp;str==0 {
        //if mouse button is clicked
        if mouse_check_button(mb_left) {
            // if mouse is over exit
            if (mouse_x&gt;=32)&amp;&amp;(mouse_x&lt;=480)&amp;&amp;(mouse_y&gt;=428)&amp;&amp;(mouse_y&lt;=428+16) {
