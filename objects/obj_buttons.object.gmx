                pref = 1;
            } else if (mouse_x&gt;=32)&amp;&amp;(mouse_x&lt;=480)&amp;&amp;(mouse_y&gt;=394)&amp;&amp;(mouse_y&lt;=394+16) {
                str = 1;
            }
        }
    }
}

//move screen to left and check values
if gend==1||pref==1||str==1 {
    view_xview[0]+=10;
    if  (view_xview[0]&gt;=room_width+10) {
        if  (gend==1) {
            game_end();
        } else if (pref==1) {
            room_goto(rm_pref);
        } else if (str==1) {
            if (audio_is_playing(snd_introsong)) {
                audio_stop_sound(snd_introsong);
            }
            room_goto(rm_game);
        }
    }
}
