            <kind>1</kind>
            <string>///click box and type
if (mouse_check_button(mb_left)) {
    if (mouse_x&gt;=32&amp;&amp;mouse_y&gt;=204&amp;&amp;mouse_x&lt;=224&amp;&amp;mouse_y&lt;224) {
        type = 1
    } else {
        type = 0;
        input = "";
    }
}

//read keyboard inputs
if  (type==1) {
    if (keyboard_check_pressed(vk_anykey)) {
        if (keyboard_lastkey&gt;=48 &amp;&amp; keyboard_lastkey&lt;=57) || (keyboard_lastkey&gt;=65 &amp;&amp; keyboard_lastkey&lt;=122) {
            if (string_length(input)&lt;22) {  //checks if character count is above 23, if so submit
                input += chr(keyboard_lastkey+32);
                audio_play_sound(choose(snd_type,snd_type1,snd_type2,snd_type3,snd_type4),0,0);
            } else {
                obj_textCont.cmd = input//send input to textCont
                type = 0;
                submit = 1;
                input = "";
            }
        } else {
            switch (keyboard_lastkey) {
            case 8: //backspace
            input = string_delete(input,string_length(input),1);
            audio_play_sound(choose(snd_type,snd_type1,snd_type2,snd_type3,snd_type4),0,0);
            break;
            case 32: //space
            input += " ";
            audio_play_sound(choose(snd_type,snd_type1,snd_type2,snd_type3,snd_type4),0,0);
            break;
            case 13: //enter
            obj_textCont.cmd = input//send input to textCont
            type = 0;
            submit = 1;
            input = "";
            audio_play_sound(choose(snd_type,snd_type1,snd_type2,snd_type3,snd_type4),0,0);
            break;
            }
        }
        
    }
}
</string>
